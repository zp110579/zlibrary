<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"

    tools:context="com.zee.libs.example.ui.activitys.MainActivity">

    <com.zee.view.ZxTextView
        android:id="@+id/tv_marquee"
        style="@style/base_btn_style_gone"
        android:text="跑马灯"
        android:visibility="visible" />

    <com.zee.view.ZxTextView
        android:id="@+id/tv_dialog"
        style="@style/base_btn_style_gone"
        android:text="Dialog" />

    <com.zee.view.ZxTextView
        android:id="@+id/tv_two_textView"
        style="@style/base_btn_style_gone"
        android:text="twoTextView" />

    <com.zee.view.ZxTextView
        android:id="@+id/tv_webView"
        style="@style/base_btn_style_gone"
        android:text="WebView" />

    <com.zee.view.ZxTextView
        android:id="@+id/tv_life"
        style="@style/base_btn_style_gone"
        android:text="Activity及Fragment生命周期监控" />

    <com.zee.view.ZxTextView
        android:id="@+id/tv_key"
        style="@style/base_btn_style_gone"
        android:text="键盘问题" />

    <com.zee.view.ZxTextView
        android:id="@+id/tv_seekbar"
        style="@style/base_btn_style_gone"
        android:text="seekBar" />

    <com.zee.view.ZxTextView
        android:id="@+id/tv_eventBus"
        style="@style/base_btn_style_gone"
        android:text="EventBus" />

    <com.zee.view.ZxTextView
        android:id="@+id/tv_eventBus_lifecycle"
        style="@style/base_btn_style_gone"
        android:text="EventBus内存信息打印" />

    <com.zee.view.ZxTextView
        android:id="@+id/tv_bubble"
        style="@style/base_btn_style_gone"
        android:text="BubbleSeekBar组件"
        android:visibility="visible" />


</LinearLayout>
